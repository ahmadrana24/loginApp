<Window x:Class="loginApp.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:loginApp"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid>
        <Image HorizontalAlignment="Left" Height="100" Margin="682,10,0,0" VerticalAlignment="Top" Width="100"/>
        <Image Margin="677,10,5,294" Source="pinterest.png"/>
        <Label x:Name="username" Content="AhmadRana" HorizontalAlignment="Left" Margin="603,32,0,0" VerticalAlignment="Top" FontWeight="Bold" RenderTransformOrigin="0.5,-0.423"/>
        <Label MouseDoubleClick="followers_MouseDoubleClick" x:Name="followers" Content="Followers : 1" HorizontalAlignment="Left" Margin="603,53,0,0" VerticalAlignment="Top" FontSize="10" RenderTransformOrigin="1.138,0.478"/>
        <Label MouseDoubleClick="following_MouseDoubleClick" x:Name="following" Content="Following : 10" HorizontalAlignment="Left" Margin="603,76,0,0" VerticalAlignment="Top" FontSize="10" RenderTransformOrigin="1.138,0.478"/>
        <Image Margin="10,32,513,320" Source="1200px-Pinterest_Logo.svg.png" Stretch="Fill"/>

        <Button Foreground="White" x:Name="seeUsers" Content="Users" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="357,53,0,0" FontStyle="Italic" FontWeight="Bold" RenderTransformOrigin="0.5,0.5" Background="Black" Click="seeUsers_Click" FontFamily="Segoe UI Semibold" />
        <Button x:Name="logout" Content="Logout" HorizontalAlignment="Left" Margin="458,53,0,0" VerticalAlignment="Top" Width="75" Click="logout_Click"/>
        <ScrollViewer HorizontalAlignment="Left" Height="279" Margin="10,125,0,0" VerticalAlignment="Top" Width="772">
            <Grid HorizontalAlignment="Left" Height="289" VerticalAlignment="Top" Width="792">
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="33*"/>
                    <ColumnDefinition Width="33*"/>
                    <ColumnDefinition Width="25*"/>
                    <ColumnDefinition Width="8*"/>
                    <ColumnDefinition Width="33*"/>

                </Grid.ColumnDefinitions>
                <Image 
               Source="33447254_1841942519433454_8706849898219175936_o.jpg" Stretch="Uniform" Grid.Column="0" Grid.Row="0"/>
                <Image  Grid.Column="1" Grid.Row="0" Source="42899412_1932518203709218_9019617559971889152_o.jpg" Stretch="Uniform"/>
                <Image 
               Source="33447254_1841942519433454_8706849898219175936_o.jpg" Stretch="Uniform" Grid.Column="2" Grid.Row="0" Grid.ColumnSpan="2"/>
                <Image  Grid.Column="4" Grid.Row="0" Source="42899412_1932518203709218_9019617559971889152_o.jpg" Stretch="Uniform"/>
                <Image 
               Source="33447254_1841942519433454_8706849898219175936_o.jpg" Stretch="Uniform" Grid.Column="1" Grid.Row="1"/>
                <Image  Grid.Column="0" Grid.Row="1" Source="42899412_1932518203709218_9019617559971889152_o.jpg" Stretch="Uniform"/>
                <Image 
               Source="33447254_1841942519433454_8706849898219175936_o.jpg" Stretch="Uniform" Grid.Column="4" Grid.Row="1"/>
                <Image  Grid.Column="2" Grid.Row="1" Source="42899412_1932518203709218_9019617559971889152_o.jpg" Stretch="Uniform" Grid.ColumnSpan="2"/>
                <Image 
               Source="33447254_1841942519433454_8706849898219175936_o.jpg" Stretch="Uniform" Grid.Column="0" Grid.Row="2"/>
                <Image  Grid.Column="1" Grid.Row="2" Source="42899412_1932518203709218_9019617559971889152_o.jpg" Stretch="Uniform"/>
                <Image 
               Source="33447254_1841942519433454_8706849898219175936_o.jpg" Stretch="Uniform" Grid.Column="2" Grid.Row="2" Grid.ColumnSpan="2"/>
                <Image  Grid.Column="4" Grid.Row="2" Source="42899412_1932518203709218_9019617559971889152_o.jpg" Stretch="Uniform"/>
            </Grid>
        </ScrollViewer>
    </Grid>
</Window>
